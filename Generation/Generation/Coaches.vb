    ''' <summary>
    ''' Determines what side of the ball they are most versed in...if a Head Coach gets fired, he will remain on the proper side of the ball---Ie, Offensive HC will become OC or Offensive Position coach, not DC or defensive position coach...
    ''' </summary>
    ''' <param name="CoachType"></param>
    ''' <returns></returns>
    Private Function GetSideOfBall(ByVal CoachType As Integer) As String
        Dim result As String = ""
        Select Case CoachType
            Case 1, 2
                Dim Num As Integer = MT.GenerateInt32(1, 100)
                If Num < 45 Then
                    result = "'OFF'"
                ElseIf Num < 91 Then
                    result = "'DEF'"
                Else
                    result = "'SPT'"
                End If
            Case 3, 7 To 13, 20, 24
                result = "'OFF'"
            Case 4, 14 To 19, 21, 25
                result = "'DEF'"
            Case 5, 6
                result = "'SPT'"
            Case 22, 23
                result = "'STC'"
        End Select
        Return result
    End Function
    ''' <summary>
    ''' Head Coaches need to be older with more experience--minimum 10 years, as do OC and DC's
    ''' </summary>
    ''' <param name="Age"></param>
    ''' <returns></returns>
    Private Function GetCoachType(Age As Integer) As Integer
        Dim Result As Integer
        Select Case Age
            Case < 29 'younger coaches start at low level positions
                Result = MT.GenerateInt32(20, 25)
                Do While Result = 22
                    Result = MT.GenerateInt32(20, 25)
                Loop
            Case < 36
                Result = MT.GenerateInt32(6, 25)
            Case Else 'Older coaches are not going to be in low level positions...
                Result = MT.GenerateInt32(1, 20)
                If Result = 20 Then Result = 22 'Strength and Conditioning coach is 22
        End Select
        Return Result
    End Function
    ''' <summary>
    ''' Cycle through the list of coaches and put them on teams
    ''' </summary>
    Public Sub PutCoachesOnTeams(ByVal CoachDT As DataTable)
        Dim GetRow As Integer
        Try
            For team As Integer = 1 To 32
                For CoachType As Integer = 1 To 25
                    GetRow = MT.GenerateInt32(0, CoachDT.Rows.Count - 1)
                    Do While (CoachDT.Rows(GetRow).Item("CoachType") <> CoachType) Or (CoachDT.Rows(GetRow).Item("TeamID") <> 0)
                        GetRow = MT.GenerateInt32(0, CoachDT.Rows.Count - 1)
                    Loop

                    CoachDT.Rows(GetRow).Item("TeamID") = team

                Next CoachType
            Next team
        Catch ex As System.InvalidCastException
            Console.WriteLine(ex.Message)
            Console.WriteLine(ex.Data)
        End Try
    End Sub

