<Window x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"   
        xmlns:wpf="clr-namespace:DotNetBrowser.WPF;assembly=DotNetBrowser"
        xmlns:local="clr-namespace:WPFFootball"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="MainWindow"
        d:DesignHeight="800"
        d:DesignWidth="1200"
        WindowState="Maximized"
        mc:Ignorable="d">
    <Grid Name="grid1">
        <wpf:WPFBrowserView Name="browserView1" FinishLoadingFrameEvent="browserView_FinishLoadingFrameEvent"></wpf:WPFBrowserView>
        <wpf:WPFBrowserView Name="browserView2"/>
    </Grid>
</Window>